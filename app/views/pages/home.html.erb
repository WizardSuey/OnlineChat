<h1>Turbochat</h1>

<% if Current.user %>
    <%= link_to "Start Chatting", rooms_path  %>
    <%= button_to "Logout", logout_path, method: :delete, class: 'btn btn-outline-secondary text' %>
<% else %>
    <%= link_to "Login", sign_in_path, class: 'nav-link text' %>
    <%= link_to "Sign Up", sign_up_path, class: 'nav-link text' %>
<% end %>